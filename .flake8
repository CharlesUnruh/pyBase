[flake8]
exclude =
    .git,
    __pycache__,
    docs/source/conf.py,
    dist,
    build,
    .mypy_cache,
    devenv

per-file-ignores =
    # F401: Import but Unused, to expose it as a module
    # F403: Unable to detect undefined names, to pull up deeper namespaces
    __init__.py: F401, F403

max-line-length = 120